        <Text layoutX="21.0" layoutY="309.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Mã nhân viên" />
        <Text layoutX="21.0" layoutY="361.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Họ tên" />
        <Text layoutX="321.0" layoutY="309.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Số điện thoại" />
        <Text layoutX="321.0" layoutY="361.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Địa chỉ" />
