            </columns>
        </TableView>
        <Button id="them" fx:id="them" layoutX="594.0" layoutY="288.0" mnemonicParsing="false" onAction="#handleInsertStaff" prefHeight="38.0" prefWidth="81.0" text="Thêm" />
        <Button id="sua" fx:id="sua" layoutX="593.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleUpdateStaff" prefHeight="38.0" prefWidth="82.0" text="Sửa" />
